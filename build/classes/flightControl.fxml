<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.GridPane?>

<AnchorPane prefHeight="1600" prefWidth="900" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="control.FlightController">
    <Label fx:id="title" alignment="TOP_CENTER" layoutX="600.0" layoutY="100.0" text="Flight Control" textAlignment="CENTER" />
    <GridPane alignment="CENTER" layoutX="700" layoutY="450">
        <Button fx:id="connect" text="Connect" GridPane.rowIndex="1" />
        <TextField fx:id="input" promptText="Degrees" GridPane.rowIndex="2" />
        <Button fx:id="pass" text="Pass" GridPane.rowIndex="3" />
        <ScrollPane prefHeight="206.0" prefWidth="187.0" GridPane.rowIndex="4">
            <Label fx:id="report" text="something" />
        </ScrollPane>
        <Button fx:id="close" text="Disconnect" GridPane.rowIndex="5" />
    </GridPane>
</AnchorPane>
