<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.GridPane?>

<?import javafx.scene.image.ImageView?>
<AnchorPane prefHeight="1600" prefWidth="900" onKeyPressed="#handleonkeytyped" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="control.FlightController">
    <Label alignment="TOP_CENTER" layoutX="600.0" layoutY="100.0" text="Flight Control" textAlignment="CENTER" />
    <GridPane alignment="CENTER" layoutX="700" layoutY="450">
        <ImageView fx:id="video" GridPane.rowIndex="1" />
        <Button text="Get up" GridPane.rowIndex="2" onAction="#getup"/>
        <Button text="Land" GridPane.rowIndex="3" onAction="#land"/>
        <ScrollPane prefHeight="206.0" prefWidth="187.0" GridPane.rowIndex="4">
            <Label fx:id="report" text="something" />
        </ScrollPane>
        <Button text="Disconnect" GridPane.rowIndex="5" onAction="#close" />
        <Button text="StartVideo" GridPane.rowIndex="6" onAction="#startVideo" />
    </GridPane>
</AnchorPane>
